{
  "workbench.colorCustomizations": {
    "activityBar.background": "#fe3cee",
    "titleBar.activeBackground": "#fe3cee",
    "titleBar.activeForeground": "#FAFCFB"
  },
  "editor.formatOnSave": true,
  "liveServer.settings.port": 5501,
  "files.exclude": {
    "**/.next": true,
    "**/node_modules": true
  },
  "restoreTerminals.terminals": [
    {
      "splitTerminals": [
        {
          "name": "client",
          "commands": [
            "cd client"
          ]
        }
      ]
    },
    {
      "splitTerminals": [
        {
          "name": "statics",
          "commands": [
            "cd statics"
          ]
        }
      ]
    }
  ]
}
